<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cutin.sample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cutin.sample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cutin.sample.SimplePickedActivity"
            android:label="SimplePicked" >
            <intent-filter>
                <action android:name="com.garlicg.cutin.action.PICK" />

                <category android:name="com.garlicg.cutin.category.RESOURCE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service
            android:name="cutin.sample.SimpleCutin"
            android:process=".simplecutin" >
            <intent-filter>
                <action android:name="cutin.sample.SimpleCutin" />
            </intent-filter>
        </service>

        <activity
            android:name="cutin.sample.IconicPickedActivity"
            android:label="IconicPicked" >
            <intent-filter>
                <action android:name="com.garlicg.cutin.action.PICK" />

                <category android:name="com.garlicg.cutin.category.RESOURCE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service
            android:name="cutin.sample.IconicCutin"
            android:icon="@drawable/ic_orange"
            android:process=".iconiccutin" >
            <intent-filter>
                <action android:name="cutin.sample.IconicCutin" />
            </intent-filter>
        </service>

        <activity
            android:name="cutin.sample.CutinIdActivity"
            android:label="CutinId" >
            <intent-filter>
                <action android:name="com.garlicg.cutin.action.PICK" />

                <category android:name="com.garlicg.cutin.category.RESOURCE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service
            android:name="cutin.sample.CutinIdCutin"
            android:process=".cutinidcutin" >
            <intent-filter>
                <action android:name="cutin.sample.CutinIdCutin" />
            </intent-filter>
        </service>

        <activity
            android:name="cutin.sample.PreferenceActivity"
            android:label="Preference" >
            <intent-filter>
                <action android:name="com.garlicg.cutin.action.PICK" />

                <category android:name="com.garlicg.cutin.category.RESOURCE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service android:name="cutin.sample.PreferenceCutin" >
            <intent-filter>
                <action android:name="cutin.sample.PreferenceCutin" />
            </intent-filter>
        </service>

        <activity
            android:name="cutin.sample.CustomDemoActivity"
            android:label="CustomDemo" >
        </activity>

        <service
            android:name="cutin.sample.CustomDemoCutin"
            android:process=".customdemocutin" />

        <activity
            android:name="cutin.sample.animation.AnimationsActivity"
            android:label="Animations" >
            <intent-filter>
                <action android:name="com.garlicg.cutin.action.PICK" />

                <category android:name="com.garlicg.cutin.category.RESOURCE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service
            android:name="cutin.sample.animation.GLSurfaceViewCutin"
            android:process=".glsurfaceview" />
        <service
            android:name="cutin.sample.animation.AnimateDrawableCutin"
            android:process=".animatedrawable" />
    </application>

</manifest>